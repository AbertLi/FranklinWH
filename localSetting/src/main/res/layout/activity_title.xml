<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="listener"
            type="com.ess.localsetting.IBackListener" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_46"
        android:background="@color/title_colors">

        <ImageButton
            android:layout_width="@dimen/qb_px_26"
            android:layout_height="@dimen/qb_px_26"
            android:layout_marginLeft="@dimen/qb_px_16"
            android:background="@mipmap/back"
            android:onClick="@{()->listener.back()}"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
